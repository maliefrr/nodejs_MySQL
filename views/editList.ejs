<div class="container">
	<div class="row">
		<div class="col-md-2"></div>
		<div class="col-md">
			<form class="mt-3" method="POST" action="/">
				<fieldset>
					<legend>Add todo List</legend>
					<div class="mb-3">
						<label for="disabledTextInput" class="form-label">Activities</label>
						<input type="text" class="form-control" placeholder="Add Your activities" name="activities" value="<%=list[0]["dataValues"].activities%>" />
					</div>
					<div class="mb-3">
						<label for="disabledSelect" class="form-label">Status</label>
						<select id="disabledSelect" class="form-select" name="status">
							<option value="1">Selesai</option>
							<option value="0">Belum Selesai</option>
						</select>
					</div>
					<button type="submit" class="btn btn-primary">Submit</button>
				</fieldset>
			</form>
		</div>
		<div class="col-md-2"></div>
	</div>
</div>
